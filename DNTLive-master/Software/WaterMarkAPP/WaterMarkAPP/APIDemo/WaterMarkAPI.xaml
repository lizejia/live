<Window x:Class="WaterMarkAPP.WaterMarkAPI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="文字水印调用" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="1" Content="文字水印调用" HorizontalAlignment="Center" VerticalAlignment="Center" Width="100" Height="50" Click="Button_Click"/>
        <Button Grid.Row="2" Content="图片水印调用" HorizontalAlignment="Center" VerticalAlignment="Center" Width="100" Height="50" Click="Button_Click_1"/>
    </Grid>
</Window>
