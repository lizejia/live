<Window x:Class="WaterWaterWaterMark.Setting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WaterWaterWaterMark" Icon="1.ico"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen" Closing="Window_Closing"
        Title="设置透明度（越往右水印越淡）" Height="100" Width="500">
    <Grid Background="Black">
        <Slider x:Name="slider" VerticalAlignment="Center" Maximum="25" Minimum="1" SmallChange="1"/>
    </Grid>
</Window>
